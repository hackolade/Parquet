{
    "name": "Parquet",
    "version": "0.2.0",
    "versionDate": "2024-04-19",
    "author": "hackolade",
    "engines": {
        "hackolade": "3.5.5",
        "hackoladePlugin": "1.2.0"
    },
    "contributes": {
        "target": {
            "applicationTarget": "PARQUET",
            "title": "Apache Parquet",
            "versions": [
                "1.x",
                "2.x"
            ]
        },
        "features": {
            "nestedCollections": false,
            "disablePatternField": false,
            "enableForwardEngineering": true,
            "disableMultipleTypes": true,
            "enableReverseEngineering": true,
            "disableChoices": true,
            "enableJsonType": true
        }
    },
    "description": "Hackolade plugin for Apache Parquet schema",
    "icon_url": "logo.png",
    "disabled": false,
    "dependencies": {
        "parquetjs-lite": "0.8.5"
    },
    "lint-staged": {
        "*.{js,json}": "prettier --write"
    },
    "simple-git-hooks": {
        "pre-commit": "npx lint-staged",
        "pre-push": "npx eslint ."
    },
    "scripts": {
        "lint": "eslint . --max-warnings=0",
        "package": "node esbuild.package.js"
    },
    "devDependencies": {
        "@hackolade/hck-esbuild-plugins-pack": "0.0.1",
        "esbuild": "0.21.4",
        "esbuild-plugin-clean": "1.0.1",
        "eslint": "9.3.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-prettier": "5.1.3",
        "lint-staged": "14.0.1",
        "prettier": "3.2.5",
        "simple-git-hooks": "2.11.1"
    }
}
